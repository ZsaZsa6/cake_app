<%= form_for @cake do |f| %>
    <%= f.label "Title of cake:" %><br>
    e.g. Bridal cake
   <br><%= f.text_field :title %><br>
    <br><%= f.label "Number of tiers on cake:" %><br>
    <br><%= f.number_field :number_tiers, in: 1...10 %><br>
    

        
    <%= f.submit %> 
    <% end %>
    <%= render 'shared/links' %>